{
  "welcomeFile": "/eh-forms/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/eh-forms/service/(.*)$",
      "target": "/service/$1",
      "destination": "dteConsentAppPortal-srv",
      "authenticationType": "none",
      "csrfProtection": false
    },
    {
      "source": "^/eh-forms/(.*)$",
      "target": "/dteconsentappclient/$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "none"
    },
    {
      "source": "^/index.html$",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa",
      "cacheControl": "no-cache, no-store, must-revalidate"
    },
    {
      "source": "^/resources/(.*)$",
      "target": "/resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    },
    {
      "source": "^/test-resources/(.*)$",
      "target": "/test-resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa",
      "cacheControl": "no-cache, no-store, must-revalidate"
    }
  ],
  "cors":[
    {
      "uriPattern": "^/eh-forms/service/(.*)$",
      "allowedOrigin": [
        {
          "host": "*.dteenergy.com",
          "port": 8443
        },
        {
          "host":"*.hana.ondemand.com",
          "port":8443
        },
        {
          "host":"*.us20.applicationstudio.cloud.sap",
          "port":8443
        }
      ]
    }
  ]
}
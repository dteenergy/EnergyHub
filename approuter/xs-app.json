{
  "welcomeFile": "/eh/consent-portal/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/admin/do/logout",
    "logoutPage": "/admin/index.html"
  },
  "routes": [
    {
      "source": "^/admin/admin/service/(.*)$",
      "target": "/admin/service/$1",
      "destination": "dteConsentAppPortal-srv",
      "authenticationType": "xsuaa",  
      "csrfProtection": false
    },
    {
      "source": "^/admin/(.*)$",
      "target": "/dteenergyadminportal/$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/consent-portal/service/(.*)$",
      "target": "/service/$1",
      "destination": "dteConsentAppPortal-srv",
      "authenticationType": "none",
      "csrfProtection": false
    },
    {
      "source": "^/eh/consent-portal/(.*)$",
      "target": "/dteconsentappclient/$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "none"
    },
    {
      "source": "^/index.html$",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa",
      "cacheControl": "no-cache, no-store, must-revalidate"
    },
    {
      "source": "^/resources/(.*)$",
      "target": "/resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    },
    {
      "source": "^/test-resources/(.*)$",
      "target": "/test-resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    },
    {
      "source": "^/eh/(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa",
      "cacheControl": "no-cache, no-store, must-revalidate"
    }
  ]
}
<mvc:View xmlns:tnt="sap.tnt" controllerName="dteenergyadminportal.controller.WelcomePage"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page" showHeader="false">
        <tnt:ToolPage>
            <tnt:sideContent>
                <tnt:SideNavigation>
                    <tnt:NavigationList id="idNavigationListMenu" itemSelect="selectedItem" selectedKey="HomePage">
                        <tnt:NavigationListItem text="Home" key="HomePage" icon="sap-icon://home"></tnt:NavigationListItem>
                        <tnt:NavigationListItem text="Enrollment Application" key="EnrollmentApplicationPage" icon="sap-icon://request"></tnt:NavigationListItem>
                        <tnt:NavigationListItem text="Consents" key="ConsentsPage" icon="sap-icon://approvals"></tnt:NavigationListItem>
                        <tnt:NavigationListItem text="EmailTemplate" key="EmailTemplatePage" icon="sap-icon://write-new-document"></tnt:NavigationListItem>
                    </tnt:NavigationList>
                </tnt:SideNavigation>
            </tnt:sideContent>
            <tnt:mainContents>
                <ScrollContainer vertical="true" width="auto" height="100%">
                    <FlexBox width="100%" height="100%" justifyContent="Center" >
                        <VBox id="mainContent" width="90%" height="100%" class="mainContentAlignment" />
                    </FlexBox>
                </ScrollContainer>
            </tnt:mainContents>
        </tnt:ToolPage>
    </Page>
</mvc:View>
{
  "name": "dteConsentAppPortal",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/postgres": "^1.10.4",
    "@sap/cds": "^8",
    "@sap/cds-dk": "^8.4.2",
    "@sap/xsenv": "^5.4.0",
    "@sap/xssec": "^4",
    "cors": "^2.8.5",
    "express": "^4",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.7.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "undeploy": "cf undeploy dteConsentAppPortal --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "watch-dteconsentappclient": "cds watch --open dteconsentappclient/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "server": {
    "cors": true
  },
  "cds": {
    "sql": {
      "native_hana_associations": false
    },
    "requires": {
      "[production]": {
        "db": "postgres",
        "auth": {
          "kind": "xsuaa",
          "restrict_all_services": false
        }
      }
    }
  }
}
